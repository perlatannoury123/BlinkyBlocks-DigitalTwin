#include <user_code.h>
#include <layer3_generic.h>
#include <microphone.h>
#include <speaker.h>
#include <light.h>
#include <abstraction.h>
#include <hwBuzzer.h>

void BBinit() {
	setColor(GREEN);
}

void BBloop() {
	setColor(GREEN);
	sleep_sec(2);
	setColor(RED);
	sleep_sec(1);
	setColor(YELLOW);
	sleep_sec(1);
	setColor(ORANGE);
	sleep_sec(2);
}

uint8_t process_standard_packet(L3_packet *packet) {
	return 0;
}

void process_standard_ack(L3_packet *p) {
}

void process_standard_unack(L3_packet *p) {
}
